@model TBADatalyticsSolutions.Models.TrainingCourse

@{
    ViewData["Title"] = "Edit Course";
}

<h2>Edit Course</h2>
<form asp-action="Edit">
    <input type="hidden" asp-for="Id" />
    <partial name="_CourseForm" />
    <button type="submit" class="btn btn-primary mt-3">Save Changes</button>
    <a class="btn btn-secondary mt-3" href="@Url.Action("Index")">Cancel</a>
</form>